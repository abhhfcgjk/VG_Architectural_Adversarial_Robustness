#%%
from PIL import Image
from pathlib import Path
import matplotlib.pyplot as plt
from torchvision.transforms.functional import resize

dataset_path = "./NIPS_test/"
resize_size_h = 498
resize_size_w = 664

#%%

image_paths = [ elem for elem in Path(dataset_path).iterdir()]
im = Image.open(image_paths[1]).convert("RGB")
plt.imshow(im)
# im

#%%
im = resize(im, (resize_size_h, resize_size_w))
plt.imshow(im)
